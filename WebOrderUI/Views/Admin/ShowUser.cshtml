
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/EasyUI/themes/icon.css" rel="stylesheet" />
    <link href="~/EasyUI/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/Css/userinfo.css" rel="stylesheet" />
    <script src="~/EasyUI/jquery.min.js"></script>
    <script src="~/EasyUI/jquery.easyui.min.js"></script>
    <script src="~/EasyUI/locale/easyui-lang-zh_CN.js"></script> 
    <script src="~/Userjs/processingajaxmsg.js"></script> 
    <script src="~/Userjs/extension.js"></script>
    <script src="~/Userjs/showuser.js"></script>
</head>
<body class="easyui-layout" data-options="fit:true">
    <div data-options="region:'north',title:'过滤条件',split:true,border:false,collapsed:true" style="height:100px;">
        <form id="searchForm">
             <table class="tableForm datagrid-toolbar" style="width:100%;height:100%;" >
                 <tr>
                     <th>用户名</th>
                     <td><input name="userName" class="easyui-textbox" style="width:312px" /> 
                     </td> 
                 </tr>
                 <tr>
                     <th>登录时间</th>
                     <td>
                         <input name="lbegin" class="easyui-datetimebox" data-options="editable:false" style="width:150px" />至<input name="lend" class="easyui-datetimebox" data-options="editable:false" style="width:150px" />
                         <a href="javascript:void(0);" class="easyui-linkbutton" data-options="iconCls:'icon-search'" onclick="_search();">查询</a>
                         <a href="javascript:void(0);" class="easyui-linkbutton" data-options="iconCls:'icon-undo'" onclick="clearSearch();">取消</a>
                     </td>
                 </tr>
             </table>
        </form>
    </div> 
    <div data-options="region:'center',border:false" style="overflow:hidden">
        <table id="datagrid" style="width:400px;height:250px"></table>
    </div>   
</body>
</html>
