


@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <!--jQuery库-->
    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
    <!--jQuery Easy库 两个样式文件，两个js脚本-->
    <link href="~/EasyUI/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/EasyUI/themes/icon.css" rel="stylesheet" />
    <!-- <script src="~/EasyUI/jquery.min.js"></script>-->
    <script src="~/EasyUI/jquery.easyui.min.js"></script>
    <script src="~/EasyUI/locale/easyui-lang-zh_CN.js"></script>
    <!--自定义库-->
    <!--<link href="~/Css/userinfo.css" rel="stylesheet" />-->
    <script src="~/Userjs/processingajaxmsg.js"></script>
    <script src="~/Userjs/extension.js"></script>
    <script src="~/Userjs/showorders.js"></script>
    <script src="~/Scripts/xzUtil.js"></script>

    <style type="text/css">
        .New_Button, .Edit_Button, .Delete_Button, .Update_Button, .Cancel_Button {
            font-size: 11px;
            color: #1B3F91;
            font-family: Verdana;
            margin-right: 5px;
        }
        .caption{
            width:80px; 
            font-size:11px;
            margin-left:0px;
        }
        .editvalue{
            width:150px;
        }
    </style>
    
</head>
<body class="easyui-layout" data-options="fit:true" style="width:100%;height:100%">
    <div data-options="region:'north',title:'过滤条件',split:true,border:false,collapsed:true" style="height:91px;">
        <form id="searchForm">
            <table class="tableForm datagrid-toolbar" style="width:100%;height:100%;">
                <tr>
                    <th>用户名</th>
                    <td>
                        <input name="userName" class="easyui-textbox" style="width:312px" />
                    </td>
                </tr>
                <tr>
                    <th>登录时间</th>
                    <td>
                        <input name="lbegin" class="easyui-datetimebox" data-options="editable:false" style="width:150px" />至<input name="lend" class="easyui-datetimebox" data-options="editable:false" style="width:150px" />
                        <a href="javascript:void(0);" id="btnQuery">查询</a>
                        <a href="javascript:void(0);" id="btnCancel">取消</a>
                    </td>
                </tr>
            </table>
        </form>
    </div>
    <div data-options="region:'center',border:false" style="overflow:hidden">
        <table id="datagrid"></table>
    </div>
    <!--订单信息--> 
    <div id="detailWindow" style="display:none;overflow:auto" > 
    <div style="width:100%;height:100%">
        <div id="orderTab" class="easyui-tabs" data-options="fit:true,border:false" style="width:100%;height:100%" >
            <div title="订单信息" data-options="fit:true">
                <div id="editform" class="form">
                    <input class="hidden" name="id" style="display:none" />
                    <table style="width:690px;">
                        <tr>
                            <td style="width:80px;">员工帐号：</td>
                            <td style="width:150px;"><input name="loginname" class="easyui-textbox editvalue" /></td>
                            <td style="width:80px;">姓名：</td>
                            <td style="width:150px;"><input name="name" class="easyui-textbox editvalue" /></td>
                            <td style="width:80px;">薪资：</td>
                            <td style="width:150px;"><input name="salary" class="easyui-textbox editvalue" /></td>
                        </tr>
                        <tr>
                            <td>性别：</td>
                            <td><input name="gender" class="easyui-combobox editvalue" /></td>
                            <td>年龄：</td>
                            <td><input name="age" class="easyui-numberspinner editvalue" minValue="0" maxValue="200" value="25" /></td>
                            <td>出生日期：</td>
                            <td><input name="birthday" class="easyui-datebox editvalue" /></td>
                        </tr>
                        <tr>
                            <td>订单号:</td>
                            <td><input id="BillCode" class="easyui-textbox editvalue"/></td>
                            <td>订单日期:</td>
                            <td><input id="OrderDate" class="easyui-datebox editvalue" /></td>
                        </tr>
                        <tr>
                            <td style="text-align:right;padding-top:5px;padding-right:20px;" colspan="6">
                                <a id="btnOk" class="Update_Button easyui-linkbutton" href="javascript:updateRow()">确定</a>
                                <a id="btnCancel" class="Cancel_Button easyui-linkbutton" href="javascript:void(0);">取消</a>
                            </td>
                        </tr>

                    </table>
                </div>
            </div>
            <div title="订单明细" data-options="fit:true">
                <table id="detaildatagrid"></table>
            </div>
        </div>
    </div>  
    </div> 

     
</body>
</html>
